@article{hogbin_public_1999,
	title = {Public Confusion Over Food Portions and Servings},
	volume = {99},
	issn = {0002-8223},
	url = {http://www.sciencedirect.com/science/article/B758G-489Y87G-D/2/be7d812197452728b0e1ecd2d8d23649},
	doi = {doi: DOI: 10.1016/S0002-8223(99)00297-7},
	abstract = {Three of 4 Americans believe there is too much conflicting information about diet (1). Part of this confusion has to do with the amount of food considered to be a “serving” or a “portion.” Each person has his or her own perceptions of “standard” food portions.

Today's large portions, the norm in many eating establishments, can differ substantially from the 3 key standards for serving sizes: the Food Guide Pyramid (2), the Nutrition Facts Label on food packaging (3), and the Exchange Lists for Meal Planning (4). For example, one large deli bagel might weigh 4 oz, whereas the half bagel listed as a serving in the Food Guide Pyramid and the Exchange Lists weighs about 1 oz. Meanwhile, the serving size listed on the Nutrition Facts Label is 1 bagel (which may vary from 1 oz to more than 4 oz). With so much variation, it is no wonder that consumers are confused about what portions and serving sizes mean and how to use them.

Many factors affect how much people eat, including a person's age, gender, activity level, cultural background, and appetite, not to mention where and when food is obtained and eaten. Most people think of a portion as the amount of a specific food they eat at a meal or for a snack. For example, spaghetti served with meat sauce is often considered a single portion, regardless of the amount served. The large portions offered at restaurants and other food venues suggest to the public that the amount served is a standard portion. In fact, these portions are often 3 to 4 times larger than what is considered standard. For example, a 3-piece chicken dinner provides 3 meat portions; a large submarine sandwich may provide 4 standard bread portions plus other sandwich components.

Several researchers have documented how inaccurate people can be when asked to estimate food portion sizes (5), (6) and (7). When keeping food records, people list foods eaten but usually estimate rather than measure or weigh actual amounts consumed. Misestimations typically result in underreporting the amount of meat, fats, sweets, and alcohol consumed. Thus, nutrient intake calculated from such data often misrepresents actual energy intake and nutrients consumed. Tools such as the Food Guide Pyramid, Nutrition Facts Label, and Exchange Lists can help consumers understand food portions, provide more accurate data for assessment, and make more healthful food choices.},
	number = {10},
	journal = {Journal of the American Dietetic Association},
	author = {{HOGBIN}, {MYRTLE} B. and {HESS}, {MARY} {ABBOTT}},
	month = oct,
	year = {1999},
	pages = {1209--1211},
	file = {hogbin.pdf:C:\Users\aha\Dropbox\UW-Research\Papers\hogbin.pdf:application/pdf}
},

@article{fitt_national_????,
