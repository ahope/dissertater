@article{arvaniti_healthy_2008,
	title = {Healthy Indexes in Public Health Practice and Research: A Review},
	volume = {48},
	issn = {1040-8398},
	shorttitle = {Healthy Indexes in Public Health Practice and Research},
	url = {http://www.informaworld.com/10.1080/10408390701326268},
	doi = {10.1080/10408390701326268},
	abstract = {The scientific community has long been interested in the overall quality of diets owing to the fact that it is important for each individual's health through a healthy, varied, and balanced diet. Much research has been conducted on methods used to measure dietary quality. These studies led to the determination of numerous indices, some very simple and some much more complex. Indices that examine diets for several attributes concurrently are able to provide a measure of overall diet quality that is not possible when only single nutrients or food groups are examined. Using these indices, diet quality may be evaluated in relation to nutrient adequacy, compliance with dietary guidelines, or nutrition recommendations, association with risk of chronic diseases, or mortality, and used to assess interventions. However, there are several factors that should be taken into consideration for the design of an index, like the purpose of its use, as well as its simplicity in daily practice. The general purpose of an index is to synthesize a large amount of information into a single useful indicator. The purpose of this review was to present and to critically review the most commonly used dietary indices, and how they reflect various aspects of diet quality. The majority of these indices fail to recognize the various inter-relationships between their components, as well as their accuracy for estimating specific health outcomes. Thus, the development of weighted dietary indices that adequately assess a dietary pattern and its relationship to the burden of a disease is considered essential.},
	number = {4},
	journal = {Critical Reviews in Food Science and Nutrition},
	author = {Arvaniti, Fotini and Panagiotakos, Demosthenes B.},
	year = {2008},
	pages = {317},
	file = {08-Arvaniti.pdf:C:\Users\aha\AppData\Roaming\Mozilla\Firefox\Profiles\8utgoj70.default\zotero\storage\T3C4DBJB\08-Arvaniti.pdf:application/pdf}
},

@article{dunton_need_2009,
	title = {The Need for Time-Intensive Information in Healthful Eating and Physical Activity Research: A Timely Topic},
	volume = {109},
	issn = {0002-8223},
	url = {http://www.sciencedirect.com/science/article/B758G-4V6SCMB-9/2/0c07efc8ad482412fd9735f4d4ee5d6d},
	doi = {doi: DOI: 10.1016/j.jada.2008.10.019},
	abstract = {Unhealthful dietary practices and physical inactivity are associated with the leading causes of mortality in the United States ( [1] and [2]). Despite well-established linkages between poor nutritional habits and sedentary behavior and a host of chronic and serious diseases, such as diabetes, heart disease, and cancer ( [3], [4] and [5]), many Americans do not meet minimum daily recommendations for the intake of essential nutrients and physical activity. Recent estimates suggest {\textless}50\% of {US} adults engage in regular physical activity based on self-reported information (6). Also, only 32.6\% of adults reportedly consume fruit two or more times per day and 27.2\% eat vegetables three or more times per day (7). To address this public health crisis, there is an urgent need to elucidate key processes underlying healthful lifestyle decisions and develop strategies to promote healthful eating and physical activity. This commentary describes how acquiring time-intensive information about microcontextual and intraindividual factors influencing daily and within-daily dietary and activity decisions is a critical step to accomplishing this goal.},
	number = {1},
	journal = {Journal of the American Dietetic Association},
	author = {Dunton, Genevieve Fridlund and Atienza, Audie A.},
	month = jan,
	year = {2009},
	pages = {30--35}
},

