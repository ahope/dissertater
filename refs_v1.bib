@article{burke_using_2008,
	title = {Using instrumented paper diaries to document self-monitoring patterns in weight loss},
	volume = {29},
	issn = {1551-7144},
	url = {http://www.sciencedirect.com/science/article/B7P72-4P8GX72-1/2/d8e1e9b31d1ec49739a917fe0bc8a408},
	doi = {doi: DOI: 10.1016/j.cct.2007.07.004},
	abstract = {Self-monitoring of eating is associated with successful weight loss, but adherence is imperfect and deteriorates over time. Moreover, intentionally or not, many individuals have difficulty keeping faithful records. We used instrumented paper previous termdiariesnext term {(IPDs)} to study self-monitoring in randomly chosen participants in the {PREFER} trial, a behavioral treatment for weight-loss study. The previous termdiariesnext term they used to self-report eating were periodically replaced with {IPDs} at various times during an 18-month weight-loss program, consisting of three successive phases: intense treatment (n = 35), less-intense treatment (n = 13), and maintenance (n = 16). We compared electronically documented self-monitoring data, showing when and how often {IPDs} were used, with self-reported data, then compared the electronically validated adherence and weight loss. Self-reported previous termdiarynext term usage exceeded {IPD-documented} usage while the electronic data demonstrated a significant decline in self-monitoring over time. previous {termDiarynext} term recording often was not timely. Percentage weight lost correlated significantly with frequency of {IPD} use (p = .001) and the number of previous termdiarynext term entries made within 15 min of opening the {IPD} (p = .002). This is the first study to document patterns of self-monitoring among participants in a weight-loss program, which demonstrated that individuals may falsify the times and frequency of self-monitoring. Furthermore, our results showed that adherence to self-monitoring and the timeliness of recording significantly correlate with improved weight loss.},
	number = {2},
	journal = {Contemporary Clinical Trials},
	author = {Burke, Lora E. and Sereika, Susan M. and Music, Edvin and Warziski, Melanie and Styn, Mindi A. and Stone, Arthur},
