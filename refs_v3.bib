@article{brown_food_2000,
	title = {A Food Display Assignment and Handling Food Models Improves Accuracy of College Students' Estimates of Food Portions},
	volume = {100},
	issn = {0002-8223},
	url = {http://www.sciencedirect.com/science/article/pii/S0002822300003096},
	doi = {10.1016/S0002-8223(00)00309-6},
	abstract = {In dietary studies, subjects' estimates of portion size are a major source of error (1). Consumers and patients who are unable to estimate food portions accurately may lack a basis for evaluating or improving their dietary intake because they do not know how much they have eaten. Furthermore, recommendations based on inaccurate information may result in inappropriate changes in intakes of energy or nutrients. Several researchers have reported that training enhanced the accuracy of persons' estimates of food portions (2), (3), (4), (5), (6), (7), (8) and (9). In various studies, subjects have been trained with actual foods (3), (6), (7), (8) and (9), 3-dimensional {(3D)} food models (2), (3), (4), (5), (7) and (8), and 2-dimensional {(2D)} food photographs (2).

When training subjects, {3D} models may be more effective because they are closer in shape and volume to real foods than {2D} photographs. However, Howat et al (2) reported that training with a combination of food models and life-sized food photographs enhanced accuracy more than the use of food models alone. The studies were limited to small groups of 4 to 26 people, with little evaluation of retention of training except at 1 and 4 weeks (5) or 4 months (6). How might training in estimation of food portions be provided in college courses with large enrollments? The purpose of this research was to determine if visually estimating food portions in a display case— a type of training more feasible for a large group—could improve college students' estimates of food portions. Additional purposes were to determine the retention of any training effect and if further exposure to {3D} or {2D} food models could substantially enhance the accuracy of estimates of food portions after viewing the food display.},
	number = {9},
	journal = {Journal of the American Dietetic Association},
	author = {Brown, Lora Beth and Oler, Charlotte Hsing-Kuan},
	month = sep,
	year = {2000},
	pages = {1063--1065}
},

