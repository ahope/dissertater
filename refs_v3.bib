@article{beasley_accuracy_2005,
	title = {Accuracy of a {PDA-based} dietary assessment program},
	volume = {21},
	issn = {0899-9007},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/15925290},
	doi = {10.1016/j.nut.2004.11.006},
	abstract = {{OBJECTIVE:} Study objectives were to assess the accuracy of a food record delivered on a personal digital assistant {(PDA)} and to examine sources of error from the {PDA-based} food record. {METHODS:} Thirty-nine adults recruited with a newspaper advertisement were trained to record food intake using {DietMatePro}, a dietary assessment program delivered on a {PDA.} After 3 d of use, subjects returned for a follow-up visit in which a 24-h recall was conducted. Subjects also were timed while recording an observed, weighed lunch. Recalled and actual food intakes were compared with estimates recorded by the subjects when using the {PDA.} Paired sample t tests and Pearson's correlations assessed means and measurements of association between {DietMatePro} data compared with the 24-h recall data and observed meal data. Bland-Altman plots were used to assess bias in food recording. Sources of error were quantified by using calories as the unit for comparison. {RESULTS:} There were no significant differences in daily totals for calories and macronutrients between {DietMatePro} data and comparison measurements. Pearson's correlations of associations between {DietMatePro} data and the comparison measurement ranged from 0.505 to 0.797 {(P} {\textless} 0.005, n = 28) for the 24-h recall and from 0.419 to 0.786 {(P} {\textless} 0.005, n = 33) for the observed lunch, depending on the nutrient measured. The largest source of absolute error in caloric estimation was attributable to portion size estimation error (49\%). {CONCLUSIONS:} {DietMatePro}, a {PDA-based} dietary assessment program, provides a method of assessing energy and macronutrient intakes comparable to the 24-h recall in samples lacking dietary restrictions.},
	number = {6},
	journal = {Nutrition {(Burbank}, Los Angeles County, Calif.)},
	author = {Beasley, Jeannette and Riley, William T and Jean-Mary, Jersino},
	month = jun,
	year = {2005},
	note = {{PMID:} 15925290},
	keywords = {Adult, Aged, Computers, Handheld, Diet Records, Dietary Carbohydrates, Dietary Fats, Dietary Proteins, Energy Intake, Female, Humans, Male, Mental Recall, Middle Aged, Nutrition Assessment, Reproducibility of Results, Sensitivity and Specificity},
	pages = {672--677},
	file = {05-Beasley.pdf:C:\Users\aha\AppData\Roaming\Mozilla\Firefox\Profiles\8utgoj70.default\zotero\storage\TXHTWKEV\05-Beasley.pdf:application/pdf;PubMed Snapshot:C:\Users\aha\AppData\Roaming\Mozilla\Firefox\Profiles\8utgoj70.default\zotero\storage\HF2FFWW4\15925290.html:text/html}
},

