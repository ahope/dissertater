	author = {Finucane, {M.M.} and Stevens, {G.A.} and Cowan, {M.J.} and Danaei, G. and Lin, {J.K.} and Paciorek, {C.J.} and Singh, {G.M.} and Gutierrez, {H.R.} and Lu, Y. and Bahalim, {A.N.} and Farzadfar, F. and Riley, {L.M.} and Ezzati, M.},
