@article{lichtenstein_diet_2006,
	title = {Diet and Lifestyle Recommendations Revision 2006: A Scientific Statement From the American Heart Association Nutrition Committee},
	volume = {114},
	shorttitle = {Diet and Lifestyle Recommendations Revision 2006},
	url = {http://circ.ahajournals.org.offcampus.lib.washington.edu/cgi/content/abstract/114/1/82},
	doi = {10.1161/CIRCULATIONAHA.106.176158},
	abstract = {Improving diet and lifestyle is a critical component of the American Heart Association's strategy for cardiovascular disease risk reduction in the general population. This document presents recommendations designed to meet this objective. Specific goals are to consume an overall healthy diet; aim for a healthy body weight; aim for recommended levels of low-density lipoprotein cholesterol, high-density lipoprotein cholesterol, and triglycerides; aim for normal blood pressure; aim for a normal blood glucose level; be physically active; and avoid use of and exposure to tobacco products. The recommendations are to balance caloric intake and physical activity to achieve and maintain a healthy body weight; consume a diet rich in vegetables and fruits; choose whole-grain, high-fiber foods; consume fish, especially oily fish, at least twice a week; limit intake of saturated fat to {\textless}7\% of energy, trans fat to {\textless}1\% of energy, and cholesterol to {\textless}300 mg/day by choosing lean meats and vegetable alternatives, fat-free (skim) or low-fat (1\% fat) dairy products and minimize intake of partially hydrogenated fats; minimize intake of beverages and foods with added sugars; choose and prepare foods with little or no salt; if you consume alcohol, do so in moderation; and when you eat food prepared outside of the home, follow these Diet and Lifestyle Recommendations. By adhering to these diet and lifestyle recommendations, Americans can substantially reduce their risk of developing cardiovascular disease, which remains the leading cause of morbidity and mortality in the United States.},
	number = {1},
	journal = {Circulation},
	author = {Lichtenstein, Alice H. and Appel, Lawrence J. and Brands, Michael and Carnethon, Mercedes and Daniels, Stephen and Franch, Harold A. and Franklin, Barry and Kris-Etherton, Penny and Harris, William S. and Howard, Barbara and Karanja, Njeri and Lefevre, Michael and Rudel, Lawrence and Sacks, Frank and Van Horn, Linda and Winston, Mary and Wylie-Rosett, Judith},
