	pages = {95 --96},
	file = {IEEE Xplore Abstract Record:C:\Users\aha\AppData\Roaming\Mozilla\Firefox\Profiles\8utgoj70.default\zotero\storage\C8IQUVG9\stamp.html:text/html;IEEE Xplore Full Text PDF:C:\Users\aha\AppData\Roaming\Mozilla\Firefox\Profiles\8utgoj70.default\zotero\storage\FT997ADT\Chen et al. - 2012 - 3D2D model-to-image registration for quantitative.pdf:application/pdf}
